<div class="container mt-3">
  <h1 class="text-center">Election Results</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Candidate</th>
        <th scope="col">Votes</th>
      </tr>
    </thead>
    <tbody>
      <% @results.each do |candidate, votes| %>
        <tr>
          <td><%= candidate %></td>
          <td><%= votes %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
